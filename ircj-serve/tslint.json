{
  "extends": "tslint:recommended",
  "rules": {
    "no-console": false,
    "quotemark": [
      true,
      "double"
    ],
    "trailing-comma": [
      true,
      {
        "singleline": "never",
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never"
        }
      }
    ],
    "semicolon": [
      true,
      "never"
    ]
  }
}